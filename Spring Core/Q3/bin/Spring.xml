<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

<bean id="bankAccountController" class="org.Q3.BankAccountController" autowire="byName"></bean>
<bean id="bankAccount" class="org.Q3.BankAccount">
<property name="accountId" value="007"></property>
<property name="accountHolderName" value="James Bond"></property>
<property name="accountType" value="Saving"></property>
<property name="accountBalance" value="1111"></property>
</bean>

</beans>